    {
      "parameters": {
        "jsCode": "const tags = ($(\"When Executed by Another Workflow\").first().json.tags ?? []);\n\nconst parts = tags.map(item => {\n  const s = String(item).trim();\n  const label = s.charAt(0).toUpperCase() + s.slice(1);\n  return `{ \"value\": \"${s}\", \"label\": \"${label}\" }`;\n});\n\n// Pretty string: each object on its own line, comma-separated\nconst tagsString = parts.join(',\\n    ');\n\nreturn [{\n  json: { tagsString }\n}];"
      },
      "type": "n8n-nodes-base.code",
      "typeVersion": 2,
      "position": [
